
@{
    ViewBag.Title = "Admin";
}

<h2>Admin</h2>

@model IEnumerable<CarInsurance.Models.Insurees>

<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email Address</th>
        <th>Quote</th>
    </tr>
    @foreach (var insuree in Model)
    {
        <tr>
            <td>@insuree.FirstName</td>
            <td>@insuree.LastName</td>
            <td>@insuree.EmailAddress</td>
            <td>@insuree.Quote</td>
        </tr>
    }
</table>

